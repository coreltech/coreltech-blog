{{ "<?xml version=\"1.0\" encoding=\"utf-8" }}?>
<rss version="2.0" xmlns:media="http://search.yahoo.com/mrss/" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
  <title>{{ .Site.Title }}</title>
  <link>{{ .Permalink }}</link>
  <description>Ãšltimas publicaciones de {{ .Site.Title }}</description>
  <generator>Coreltech IT Solutions</generator>
  <language>es</language>
  <lastBuildDate>{{ now.Format "Mon, 02 Jan 2006 15:04:05 -0700" }}</lastBuildDate>
  <atom:link href="{{ .Permalink }}" rel="self" type="application/rss+xml" />

  {{ range .Site.RegularPages.ByDate.Reverse }}
  <item>
    <title>{{ .Title }}</title>
    <link>{{ .Permalink }}</link>
    <pubDate>{{ .Date.Format "Mon, 02 Jan 2006 15:04:05 -0700" }}</pubDate>
    <guid>{{ .Permalink }}</guid>
    <description>{{ .Summary | html }}</description>
    <media:content url="{{ .RelPermalink }}{{ .Params.thumbnail }}" type="image/webp" />
  </item>
  {{ end }}
</channel>
</rss>
