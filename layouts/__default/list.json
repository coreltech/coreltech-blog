[
  {{ range $index, $page := .Site.RegularPages.ByDate.Reverse }}
    {
      "title": "{{ .Title }}",
      "link": "{{ .RelPermalink }}",
      "date": "{{ .Date.Format "2006-01-02" }}",
      "thumbnail": "{{ if .Params.thumbnail }}{{ .RelPermalink }}{{ .Params.thumbnail }}{{ end }}"
    }{{ if lt $index 2 }},{{ end }}
  {{ end }}
]
